				"lastPublishTime": "2024-01-04T23:50:56Z"
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/DSblobsource')]",
				"[concat(variables('factoryId'), '/datasets/DSblobsink')]"

{
	"name": "dataflowWindowTransformation001",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "OrdersTable",
						"type": "DatasetReference"
					},
					"name": "sourceOrdersTable"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "window1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          OrderID as integer,",
				"          ProductName as string,",
				"          OrderDate as timestamp,",
				"          Quantity as integer,",
				"          ProductPrice as decimal(10,2)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table',",
				"     partitionBy('hash', 1)) ~> sourceOrdersTable",
				"sourceOrdersTable window(over(ProductName),",
				"     desc(ProductPrice, true),",
				"     denserank = denseRank(),",
				"     partitionBy('hash', 1)) ~> window1"
			]
		}
	}
}